{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        "./*"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@dev/*": [
        "src/dev/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@common/*": [
        "src/components/common/*"
      ],
      "@layout/*": [
        "src/components/layout/*"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@queries/*": [
        "src/hooks/queries/*"
      ],
      "@mutations/*": [
        "src/hooks/mutations/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@views/*": [
        "src/views/*"
      ]
    },
    "types": [
      "vite/client",
      "@cloudflare/workers-types"
    ]
  },
  "include": [
    "src",
    "functions"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}